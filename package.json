{
  "name": "n8n-nodes-crowd-bf-explorer",
  "version": "1.3.31",
  "description": "n8n node for Cardano blockchain exploration. Simple API wrapper to interack with Blockfrost APIs",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lintfix": "eslint . --fix"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BlockfrostApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CrowdBfExplorer/CrowdBfExplorer.node.js",
      "dist/nodes/CrowdBfExplorer/AdaHandleResolver.node.js",
      "dist/nodes/CrowdBfExplorer/DEXTrade.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "cardano",
    "blockchain",
    "ada"
  ],
  "license": "MIT",
  "homepage": "https://n8n.io",
  "author": {
    "name": "AdaCow",
    "email": "patrick@crowdtrendz.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crowdtrendz/n8n-nodes-crowd-bf-explorer.git"
  },
  "engines": {
    "node": ">=16.10",
    "pnpm": ">=7.18"
  },
  "packageManager": "pnpm@7.18.0",
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@blockfrost/blockfrost-js": "^5.4.0",
    "@emurgo/cardano-serialization-lib-nodejs": "^12.0.0",
    "n8n-workflow": "^1.0.0"
  }
}
